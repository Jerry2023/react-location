{
  "name": "react-location-code",
  "version": "1.1.3",
  "description": "The tool for locating React source code",
  "module": "dist/index.esm.js",
  "main": "dist/index.cjs.js",
  "author": "jerry2023",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jerry2023/react-location"
  },
  "bugs": {
    "url": "https://github.com/Jerry2023/react-location/issues"
  },
  "homepage": "https://github.com/Jerry2023/react-location/blob/master/README.md",
